module Muxes4to1(

	input [7:0] A,
	input [7:0] B,
	input [7:0] C,
	input [7:0] D,
	input [1:0] S,
	
	output reg [7:0] OUT
);

	
	always @(*) begin
		case (S)
		2'b00 : 
			OUT = A;
		2'h1 : //Cambiamos entre bases al comparar
			OUT = B;
		2'd2 : 
			OUT = C;
		2'b11 : 
			OUT = D;
		
		default:
			OUT = 8'hxx;
		endcase
	end
	
	endmodule
	//Es necesario un enter despu√©s de esto para que se 
	// ponga en color azul 